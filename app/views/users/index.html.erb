<%= render 'books/user', user:@user %>
<%= render 'books/book', book:@book %>

<h1>Users</h1>
<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= image_tag user.get_profile_image(100,100) %></td>
        <td><%= user.name %></td>
        <td><%= link_to "show",user_path(user) %>
      </tr>
    <% end %>
  </tbody>
</table>

