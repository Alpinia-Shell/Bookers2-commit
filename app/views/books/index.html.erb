<h3>User info</h3>
<%= image_tag @user.get_profile_image(100,100) %>
<%= @user.name %>
<%= @user.introduction %>
<%= link_to "Setting",edit_user_registration_path(@user) %>

<h3>New Book</h3>
<%= form_with model: @book do |f| %>
  <%= f.text_field :title %>
  <%= f.text_area :body %>
  <%= f.submit 'Create Book' %> 
<% end %>
